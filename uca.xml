<action>
	<icon>state-warning</icon>
	<name>Secure Erase File</name>
	<submenu></submenu>
	<unique-id>1746781126882232-4</unique-id>
	<command>bash -c &apos;p=&quot;$0&quot;; if [ -f &quot;$p&quot; ]; then tmp=&quot;/tmp/$(basename &quot;$p&quot;)_$$.tar&quot;; sudo tar -cf &quot;$tmp&quot; &quot;$p&quot; &amp;&amp; sudo dd if=/dev/urandom of=&quot;$tmp&quot; bs=1M count=3 status=none &amp;&amp; sudo rm -f &quot;$p&quot;; elif [ -d &quot;$p&quot; ]; then tmp=&quot;/tmp/$(basename &quot;$p&quot;)_$$.tar&quot;; sudo tar -cf &quot;$tmp&quot; -C &quot;$(dirname &quot;$p&quot;)&quot; &quot;$(basename &quot;$p&quot;)&quot; &amp;&amp; sudo dd if=/dev/urandom of=&quot;$tmp&quot; bs=1M count=10 status=none &amp;&amp; sudo rm -rf &quot;$p&quot;; fi&apos; %f</command>
	<description></description>
	<range>*</range>
	<patterns>*</patterns>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
